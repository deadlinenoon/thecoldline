{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.thecoldline.com" }],
      "destination": "https://thecoldline.com/$1",
      "permanent": true
    }
  ],
  "crons": [
    { "path": "/api/travel/run", "schedule": "1 4 * * 2" },
    { "path": "/api/travel/run", "schedule": "1 5 * * 2" }
  ],
  "env": {}
}
