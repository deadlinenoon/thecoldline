{
  "name": "thecoldline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy:prebuilt": "vercel deploy --prebuilt --prod --yes",
    "type-check": "tsc -noEmit",
    "make:pdf": "node scripts/make-tutorial-pdf.js",
    "make:video": "node scripts/make-tutorial-video.js",
    "check:agent": "node scripts/agent-parity.ts || node scripts/agent-parity.js",
    "guard": "node scripts/no-mock-check.mjs",
    "typecheck": "tsc --noEmit",
    "export:travel:2025": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/export-travel-miles-2025.ts",
    "tag:prod": "node -e \"const d=new Date().toISOString().replace(/[-:T]/g,'').slice(0,13).replace(/(.{8})(.{2})/,'$1-$2');console.log('prod-'+d)\" | xargs -I {} sh -c 'git tag -a {} -m Production && git push origin {}'",
    "vercel:rollback": "vercel rollback",
    "vercel:ls": "vercel ls || vercel list",
    "vercel:promote": "node -e \"console.log('Use: npm run vercel:promote <deployment-url>')\""
  },
  "dependencies": {
    "@upstash/redis": "^1.20.0",
    "@vercel/analytics": "^1.5.0",
    "geographiclib": "^1.52.2",
    "luxon": "^3.4.4",
    "next": "15.5.2",
    "pdfkit": "^0.17.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^2.13.3",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/forms": "^0.5.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "postcss": "^8.5.6",
    "puppeteer": "^23.7.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5",
    "ts-node": "^10.9.2"
  }
}
